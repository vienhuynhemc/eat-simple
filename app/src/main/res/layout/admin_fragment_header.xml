<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app1="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="65dp"
    android:layout_height="match_parent"
    android:background="@color/white">

    <TextView
        android:id="@+id/icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="5dp"
        android:textColor="#4162fb"
        android:textSize="20sp"
        app1:layout_constraintEnd_toEndOf="parent"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:layout_width="25dp"
        android:layout_height="2dp"
        android:layout_marginTop="28dp"
        app1:cardBackgroundColor="#f5f5f5"
        app1:cardElevation="0dp"
        app1:layout_constraintEnd_toEndOf="parent"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toBottomOf="@id/icon" />

    <TextView
        android:id="@+id/icon_home"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginTop="31dp"
        android:gravity="center"
        android:paddingEnd="15dp"
        android:paddingBottom="15dp"
        android:textColor="#4162fb"
        android:textSize="20sp"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toBottomOf="@id/icon"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/icon_danhMuc"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:gravity="center"
        android:paddingEnd="15dp"
        android:paddingBottom="15dp"
        android:textColor="#4162fb"
        android:textSize="20sp"
        android:translationY="-25dp"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toBottomOf="@id/icon_home"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/icon_MaGiamGia"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/admin_background_select"
        android:gravity="center"
        android:paddingEnd="15dp"
        android:paddingBottom="15dp"
        android:textColor="#4162fb"
        android:textSize="20sp"
        android:translationY="-50dp"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toBottomOf="@id/icon_danhMuc"
        tools:ignore="RtlSymmetry" />

    <TextView
        android:id="@+id/icon_ThongTinCaNhan"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/admin_background_select"
        android:gravity="center"
        android:paddingEnd="15dp"
        android:paddingBottom="15dp"
        android:textColor="#4162fb"
        android:textSize="20sp"
        android:translationY="-75dp"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toBottomOf="@id/icon_MaGiamGia"
        tools:ignore="RtlSymmetry" />


    <androidx.cardview.widget.CardView
        android:layout_width="25dp"
        android:layout_height="2dp"
        android:translationY="-85dp"
        app1:cardBackgroundColor="#f5f5f5"
        app1:cardElevation="0dp"
        app1:layout_constraintEnd_toEndOf="parent"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toBottomOf="@id/icon_ThongTinCaNhan" />

    <androidx.cardview.widget.CardView
        android:id="@+id/log_out"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginBottom="30dp"
        android:foregroundGravity="center"
        android:translationY="-57dp"
        app1:cardBackgroundColor="@color/color_admin_main"
        app1:cardCornerRadius="21dp"
        app1:cardElevation="0dp"
        app1:layout_constraintEnd_toEndOf="parent"
        app1:layout_constraintStart_toStartOf="parent"
        app1:layout_constraintTop_toBottomOf="@id/icon_ThongTinCaNhan">

        <TextView
            android:id="@+id/icon_logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:paddingStart="3dp"
            android:textColor="@color/white"
            android:textSize="17sp"
            tools:ignore="RtlSymmetry" />

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>